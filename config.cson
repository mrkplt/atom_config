"*":
  "atom-ctags":
    buildTimeout: 60000
    cmdArgs: "--exclude=.git --exclude=data"
  "atom-terminal": {}
  core:
    audioBeep: false
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
  "custom-title": {}
  editor:
    fontFamily: "Fira Mono OT"
    fontSize: 13
    invisibles: {}
    showInvisibles: true
  "exception-reporting":
    userId: "245b978d-b696-a225-fc21-88c901bb05fc"
  "git-blame": {}
  "git-diff":
    showIconsInEditorGutter: true
  "git-history": {}
  tabs:
    usePreviewTabs: true
  "trailing-spaces":
    enableForCursorLines: true
    enableForIndentation: true
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
